<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <FrameLayout
        android:id="@+id/FramFragment"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/llManu"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:id="@+id/llManu"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_45sdp"
        android:background="@color/white"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:baselineAligned="false">
        <RelativeLayout
            android:id="@+id/menu_home"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:onClick="onClick">

            <ImageView
                android:id="@+id/ivHome"
                android:layout_width="@dimen/_18sdp"
                android:layout_height="@dimen/_18sdp"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="@dimen/_10sdp"
                android:adjustViewBounds="true"
                android:contentDescription="@null"
                android:src="@drawable/ic_home" />

            <com.ikksa.customfonts.MyTextView_Light
                android:id="@+id/tvHome"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/ivHome"
                android:gravity="center_horizontal"
                android:padding="3dp"
                android:text="@string/home"
                android:textAppearance="@style/PoppinsRegular"
                android:textColor="@color/black"
                android:textSize="@dimen/_8ssp" />
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/menu_fav"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:onClick="onClick">

            <ImageView
                android:id="@+id/ivFav"
                android:layout_width="@dimen/_18sdp"
                android:layout_height="@dimen/_18sdp"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="@dimen/_10sdp"
                android:adjustViewBounds="true"
                android:contentDescription="@null"
                android:src="@drawable/ic_wish" />

            <com.ikksa.customfonts.MyTextView_Light
                android:id="@+id/tvWishlist"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/ivFav"
                android:gravity="center_horizontal"
                android:padding="3dp"
                android:text="@string/wishlist"
                android:textAppearance="@style/PoppinsRegular"
                android:textColor="@color/black"
                android:textSize="@dimen/_8ssp" />

        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/menu_cart"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:onClick="onClick">

            <ImageView
                android:id="@+id/ivCart"
                android:layout_width="@dimen/_18sdp"
                android:layout_height="@dimen/_18sdp"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="@dimen/_10sdp"
                android:adjustViewBounds="true"
                android:contentDescription="@null"
                android:src="@drawable/ic_bag" />

            <com.ikksa.customfonts.MyTextView_Light
                android:id="@+id/tvCart"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/ivCart"
                android:gravity="center_horizontal"
                android:padding="3dp"
                android:text="@string/bag"
                android:textAppearance="@style/PoppinsRegular"
                android:textColor="@color/black"
                android:textSize="@dimen/_8ssp" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/menu_doce"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:onClick="onClick">

            <ImageView
                android:id="@+id/ivDoce"
                android:layout_width="@dimen/_18sdp"
                android:layout_height="@dimen/_18sdp"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="@dimen/_10sdp"
                android:adjustViewBounds="true"
                android:contentDescription="@null"
                android:src="@drawable/ic_shipping" />

            <com.ikksa.customfonts.MyTextView_Light
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/ivDoce"
                android:gravity="center_horizontal"
                android:padding="3dp"
                android:text="@string/Orders"
                android:textAppearance="@style/PoppinsRegular"
                android:textColor="@color/black"
                android:textSize="@dimen/_8ssp"
                android:id="@+id/tvOrder"/>

        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/menu_profile"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:onClick="onClick">

            <ImageView
                android:id="@+id/ivProfile"
                android:layout_width="@dimen/_18sdp"
                android:layout_height="@dimen/_18sdp"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="@dimen/_10sdp"
                android:adjustViewBounds="true"
                android:contentDescription="@null"
                android:src="@drawable/ic_user" />

            <com.ikksa.customfonts.MyTextView_Light
                android:id="@+id/tvProfile"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/ivProfile"
                android:gravity="center_horizontal"
                android:padding="3dp"
                android:text="@string/profile"
                android:textAppearance="@style/PoppinsRegular"
                android:textColor="@color/black"
                android:textSize="@dimen/_8ssp" />
        </RelativeLayout>

    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>